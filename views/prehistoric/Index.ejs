<form action="/prehistoric_creatures" method="GET">
    <label for="typeFilter">Filter By Type</label>
    <input type="text" name="typeFilter" id="typeFilter">
    <input type="submit">
</form>

<ul>
    <% creaturesData.forEach((creature, index)=>{ %>
        <li>
            <h1><%= creature.type %></h1> <img src="<%= creature.img_url %>" width="300px" height ="250px">  
            <form action="/prehistoric_creatures/<%= index %>/?_method=DELETE " method="POST">
                <input type="submit" value="delete"></br>
            </form>
        </li>
   <% }) %>

</ul>